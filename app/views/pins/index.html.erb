<h1>Listing pins</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Process</th>
      <th>Paper</th>
      <th>Year</th>
      <th>Release date</th>
      <th>Ebeans</th>
      <th>Paypal</th>
      <th>Size</th>
      <th>Run size</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= pin.image %></td>
        <td><%= pin.name %></td>
        <td><%= pin.process %></td>
        <td><%= pin.paper %></td>
        <td><%= pin.year %></td>
        <td><%= pin.release_date %></td>
        <td><%= pin.ebeans %></td>
        <td><%= pin.paypal %></td>
        <td><%= pin.size %></td>
        <td><%= pin.run_size %></td>
        <td><%= link_to 'Show', pin %></td>
        <td><%= link_to 'Edit', edit_pin_path(pin) %></td>
        <td><%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pin', new_pin_path %>
